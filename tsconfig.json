{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "removeComments": true,
    "paths": {
      "init-cli-loaders": ["./test/init-cli-loader.test.ts"]
    }
  },
  "exclude": ["node_modules", "website", "experimental"]
}
